$TTL 1m

@ SOA servera.{{ example_dns_name }}. root.servera.{{ example_dns_name }}. ( 2019083000 1h 10m 1w 10m )
@ NS servera.{{ example_dns_name }}.

; DNS records for kerberos
ldap                  CNAME servera
_ldap._tcp            SRV 0 0 389 ldap
_ldaps._tcp           SRV 0 0 636 ldap
kerberos              CNAME servera
_kerberos             TXT "{{ example_dns_name | upper }}"
_kerberos._udp        SRV 0 0 88 kerberos
_kerberos-master._udp SRV 0 0 88 kerberos
_kerberos-adm._tcp    SRV 0 0 749 kerberos
_kpasswd._udp         SRV 0 0 464 kerberos


